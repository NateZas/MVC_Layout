@model List<MVC_Layout.Models.Brand>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Brand</h2>
<table class="table">
    <thead>
        <tr>
            <td>Brand ID</td>
            <td>Brand Name</td>
        </tr>
    </thead>
    <tbody>

        @foreach (var cat in Model)
        {
            <tr>
                <td>@cat.BrandID</td>
                <td>@cat.BrandName</td>
            </tr>
        }
    </tbody>
</table>

