
@{
    ViewBag.Title = "Student";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Student</h2>

<table class="table table-bordered w-50">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Grade</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.Student)
        {
            <tr>

                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Grade</td>
                
            </tr>
        }
        </tbody>
</table>